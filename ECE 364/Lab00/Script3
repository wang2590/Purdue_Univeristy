#! /bin/bash
Num_Of_Param=$#
Param_Valus=$@
#echo"Script 2 $Param_Values"
for (( I=$Param_Valus; I >= 0; I-- ))
do
    if (( $I%3 == 0 ))
    then
        echo "$I"
    fi
done

#echo "The program received $Num_Of_Param parameters"
#echo "The parameters are: "
#echo "$Param_Values"
#echo "The 2nd parameter is \"$2\""
#echo "The 4th paramter is \"$4\""
exit 0
